<div class="container">
<% uri = URI.parse(request.original_url)
        user_list = []
        uris = uri.to_s
%>
  <!-- Page Heading -->
  <% if uris.include? "?style=vestiaire" %>
    <div style="height:20px"></div>
  <% else %>
    <div style="text-align:right;height:50px">
      <div style="margin-top:6px">
      Classer : <%= link_to 'Ã€ la une', root_path, class:'link_bar' %> | <%= link_to 'date', '?filter=1', class:'link_bar' %>
      </div>
    </div>
  <% end %>

<% if uris.include? "?style=activ" or uris.include? "?style=vestiaire" %>
  <% puts("ok") %>
<% else %>
    <div class="card-columns card-columns-chrome">
      <%= render "top_left" %>
      <%= render "top_middle" %>
      <%= render "top_right" %>
    </div>
<% end %>

   <% if uris.include? "?style=activ" %>
      <%= render 'newsfeed' %>
   <% elsif uris.include? "?style=vestiaire" %>  
      <%= render 'vestiaire' %>
   <% else %>
      <%= render 'all' %>
   <% end %>
 
  <!-- /.row -->
</div>
    <!-- /.container -->